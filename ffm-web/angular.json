{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ffm-web": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/ffm-web",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "src/assets",
                            "src/favicon.ico"
                        ],
                        "styles": [
                            "src/styles.css",
                            "src/assets/web/assets/mobirise-icons/mobirise-icons.css",
                            "src/assets/bootstrap/css/bootstrap.min.css",
                            "src/assets/bootstrap/css/bootstrap-grid.min.css",
                            "src/assets/bootstrap/css/bootstrap-reboot.min.css",
                            "src/assets/tether/tether.min.css",
                            "src/assets/dropdown/css/style.css",
                            "src/assets/animatecss/animate.min.css",
                            "src/assets/socicon/css/styles.css",
                            "src/assets/theme/css/style.css",
                            "src/assets/mobirise/css/mbr-additional.css",
                            "src/assets/datatables/data-tables.bootstrap4.min.css"
                        ],
                        "scripts": [
                            "src/assets/web/assets/jquery/jquery.min.js",
                            "src/assets/popper/popper.min.js",
                            "src/assets/bootstrap/js/bootstrap.min.js",
                            "src/assets/tether/tether.min.js",
                            "src/assets/smoothscroll/smooth-scroll.js",
                            "src/assets/dropdown/js/nav-dropdown.js",
                            "src/assets/dropdown/js/navbar-dropdown.js",
                            "src/assets/touchswipe/jquery.touch-swipe.min.js",
                            "src/assets/viewportchecker/jquery.viewportchecker.js",
                            "src/assets/parallax/jarallax.min.js",
                            "src/assets/theme/js/script.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "staging": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.staging.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "ffm-web:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "ffm-web:build:production"
                        },
                        "staging": {
                            "browserTarget": "ffm-web:build:staging"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "ffm-web:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.css"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "ffm-web:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "ffm-web:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "ffm-web",
    "cli": {
        "analytics": false
    }
}
